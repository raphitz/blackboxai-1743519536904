# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=eventoando-dev

# Environment
ENVIRONMENT=dev
PROJECT_NAME=eventoando

# Frontend Configuration
FLUTTER_WEB_PORT=8000
FLUTTER_WEB_HOSTNAME=0.0.0.0

# API Configuration
API_URL=https://api-dev.eventoando.com
API_VERSION=v1

# Database Configuration
DB_HOST=eventoando-dev.cluster-xxxxx.region.rds.amazonaws.com
DB_PORT=3306
DB_NAME=eventoando
DB_USERNAME=admin
DB_PASSWORD=dev_password_change_me

# DynamoDB Tables
EVENTS_TABLE=eventoando-events-dev
COUPONS_TABLE=eventoando-coupons-dev

# S3 Buckets
FRONTEND_BUCKET=eventoando-frontend-dev
COUPONS_BUCKET=eventoando-coupons-dev

# Cognito
COGNITO_USER_POOL_ID=us-east-1_xxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_REGION=us-east-1

# CloudFront
CLOUDFRONT_DISTRIBUTION_ID=XXXXXXXXXX

# EventBridge
EVENT_BUS_NAME=eventoando-dev

# Logging
LOG_LEVEL=debug
ENABLE_DEBUG_LOGGING=true
ENABLE_ACCESS_LOGGING=true

# Feature Flags
ENABLE_BETA_FEATURES=true
ENABLE_PERFORMANCE_MONITORING=true

# Cache Configuration
CACHE_TTL=3600
ENABLE_REDIS_CACHE=false

# Security
JWT_SECRET=dev_jwt_secret_change_me
ENABLE_SSL=true
CORS_ALLOWED_ORIGINS=http://localhost:8000,https://dev.eventoando.com

# Monitoring
ENABLE_CLOUDWATCH_METRICS=true
ENABLE_XRAY_TRACING=true

# Rate Limiting
API_RATE_LIMIT=100
API_RATE_LIMIT_WINDOW=60

# Notification Settings
ENABLE_EMAIL_NOTIFICATIONS=true
ENABLE_SMS_NOTIFICATIONS=false
NOTIFICATION_FROM_EMAIL=noreply@dev.eventoando.com

# Integration Settings
PAYMENT_GATEWAY_URL=https://payment-sandbox.example.com
PAYMENT_GATEWAY_API_KEY=sandbox_key_change_me

# Performance
LAMBDA_MEMORY_SIZE=128
LAMBDA_TIMEOUT=30
DB_MAX_CONNECTIONS=10
DB_CONNECTION_TIMEOUT=5

# Backup Configuration
ENABLE_AUTOMATED_BACKUPS=true
BACKUP_RETENTION_DAYS=7

# Development Tools
ENABLE_SWAGGER_UI=true
ENABLE_GRAPHIQL=true